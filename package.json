{
  "private": true,
  "name": "mnmlivan.xyz",
  "version": "1.0.0",
  "description": "Personal web site built using the Eleventy site generator.",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve",
    "update": "yarn upgrade-interactive --latest && yarn upgrade",
    "format": "prettier --write **/*.{css,js,json}"
  },
  "dependencies": {
    "@11ty/eleventy": "github:11ty/eleventy",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-plugin-bundle": "^1.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "browserslist": "^4.21.5",
    "dotenv": "^16.0.3",
    "html-minifier": "^4.0.0",
    "lightningcss": "^1.18.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "sanitize-html": "^2.9.0"
  },
  "devDependencies": {
    "prettier": "^2.8.3"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mnmlivan/mnmlivan.xyz.git"
  },
  "bugs": {
    "url": "https://github.com/mnmlivan/mnmlivan.xyz/issues"
  },
  "author": {
    "name": "mnmlivan",
    "email": "mnmlivan@pm.me",
    "url": "https://mnmlivan.xyz"
  },
  "license": "MIT",
  "homepage": "https://github.com/mnmlivan/mnmlivan.xyz#readme"
}
